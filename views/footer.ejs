<div id="footer">
    <p>
    <%  //logged in
    if(typeof(user) == 'object' && user != null){  
        
        %>
        <p class="pure-button">Github user: <%= user.displayName %></p>
        <a href="/logout?last=<%= name %>" class="pure-button pure-button-error">Log out from github</a> 
    <% } else { 
        //not logged in 
    %>
        <a href="/auth/github?last=<%= name %>" class="pure-button pure-button-primary">Login with github </a><br/>
    <% } %>
    </p>
</div>

<script type="text/javascript">
        /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
        var disqus_shortname = 'apiatlas'; // required: replace example with your forum shortname

        /* * * DON'T EDIT BELOW THIS LINE * * */
        (function() {
            var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
            dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
            (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
        })();
    </script>


